<p id="notice"><%= notice %></p>

<h1>Animal Subtypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Animal type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_subtypes.each do |animal_subtype| %>
      <tr>
        <td><%= animal_subtype.name %></td>
        <td><%= animal_subtype.animal_type_id %></td>
        <td><%= link_to 'Show', animal_subtype %></td>
        <td><%= link_to 'Edit', edit_animal_subtype_path(animal_subtype) %></td>
        <td><%= link_to 'Destroy', animal_subtype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Animal Subtype', new_animal_subtype_path %>
